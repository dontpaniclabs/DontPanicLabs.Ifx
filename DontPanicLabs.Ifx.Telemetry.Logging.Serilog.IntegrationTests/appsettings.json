{
  "skipEnvironmentVariables": true,
  "appSettings": {},
  "ifx": {
    "telemetry": {
      "logging": {
        "serilog": {
          "Using": [
            "Serilog.Sinks.MSSqlServer",
            "Serilog.Sinks.File",
            "Serilog.Sinks.Console"
          ],
          "MinimumLevel": "Verbose",
          "WriteTo": [
            {
              "Name": "MSSqlServer",
              "Args": {
                "connectionString": "Server=localhost,1433;Database=master;User Id=sa;Password=YourStrongPassw0rd;TrustServerCertificate=True;",
                "sinkOptionsSection": {
                  "tableName": "MyLogTable",
                  "schemaName": "dbo",
                  "autoCreateSqlTable": true,
                  "batchPostingLimit": 50,
                  "batchPeriod": "0.00:00:05"
                }
              }
            },
            {
              "Name": "File",
              "Args": {
                "formatter": "Serilog.Formatting.Json.JsonFormatter, Serilog",
                "path": "logs/serilog-test-.log",
                "rollingInterval": "Day",
                "retainedFileCountLimit": 7
              }
            },
            {
              "Name": "Console",
              "Args": {
                "formatter": "Serilog.Formatting.Json.JsonFormatter, Serilog"
              }
            }
          ]
        }
      }
    }
  }
}